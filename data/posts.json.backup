    "content": "{\"time\":1633024800000,\"blocks\":[{\"type\":\"header\",\"data\":{\"text\":\"Welcome to the New Blog System\",\"level\":1}},{\"type\":\"paragraph\",\"data\":{\"text\":\"This is the first post in our new fullstack blog system. We've moved from an external Django backend to a fully integrated Next.js solution.\"}},{\"type\":\"paragraph\",\"data\":{\"text\":\"Benefits of the new system:\"}},{\"type\":\"list\",\"data\":{\"style\":\"unordered\",\"items\":[\"Real-time data updates\",\"Better performance\",\"Simplified deployment\",\"Integrated internationalization\"]}},{\"type\":\"paragraph\",\"data\":{\"text\":\"Stay tuned for more updates!\"}}],\"version\":\"2.28.2\"}",
